{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/jobFilter.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "minSalary": {
      "type": "integer",
      "minimum": 0
    },
    "equity": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "title",
    "companyHandle",
    "salary"
  ]
}
